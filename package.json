{
  "name": "onlyflans-web",
  "version": "1.0.0",
  "description": "Dashboard admin para Only Flans - Plataforma de reclutamiento",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "prepare": "husky"
